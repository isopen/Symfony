{% set count = -1 %}
{% set i = 1 %}
{% for result in results %}
  {% set back_count = result.count %}
  {% if count != back_count %}
    <div class="row"><b>{{ i }} место</b></div>
    {% set count = back_count %}
    {% set i = i + 1 %}
  {% endif %}
  <div class="row"><div>{{ result.name }} {{ result.count }}</div>
{% endfor %}